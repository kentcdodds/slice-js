{
  "name": "slice-js",
  "version": "0.0.0-semantically-released",
  "private": true,
  "description": "A tool to generate a program slice of your JavaScript code",
  "main": "dist/index.js",
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\""
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.1.0",
    "indent-string": "^3.0.0",
    "lodash": "^4.16.6"
  },
  "devDependencies": {
    "all-contributors-cli": "^3.0.6",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "babel-template": "^6.16.0",
    "codecov": "^1.0.1",
    "combs": "^0.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.9.1",
    "eslint-config-kentcdodds": "^11.1.0",
    "glob": "^7.1.1",
    "husky": "^0.11.7",
    "istanbul-lib-instrument": "^1.2.0",
    "jest-cli": "^17.0.3",
    "match-sorter": "^1.6.0",
    "opt-cli": "^1.5.1",
    "p-s": "^3.0.1",
    "pify": "^2.3.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.2.1",
    "spawn-command": "^0.0.2",
    "strip-indent": "^2.0.0",
    "validate-commit-msg": "^2.8.0"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/jest"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "/helpers/",
      "/integration/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 95,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/slice-js.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/slice-js/issues"
  },
  "homepage": "https://github.com/kentcdodds/slice-js#readme"
}
