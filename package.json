{
  "name": "slice-js",
  "version": "0.0.0-semantically-released",
  "description": "A tool to generate a program slice of your JavaScript code",
  "main": "dist/index.js",
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\""
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.14.0",
    "babel-plugin-istanbul": "2.0.1",
    "babel-plugin-minify-dead-code-elimination": "0.0.1",
    "indent-string": "3.0.0",
    "lodash": "4.15.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^3.0.6",
    "babel-cli": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-node6": "11.0.0",
    "babel-register": "6.14.0",
    "codecov": "^1.0.1",
    "combs": "0.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.4.0",
    "eslint-config-kentcdodds": "^10.1.0",
    "glob": "7.0.6",
    "husky": "^0.11.7",
    "jest-cli": "15.1.1",
    "opt-cli": "^1.5.1",
    "p-s": "^3.0.1",
    "pify": "2.3.0",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.2.1",
    "spawn-command": "0.0.2-1",
    "validate-commit-msg": "^2.8.0"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/jest"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "/helpers/"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/slice-js.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/slice-js/issues"
  },
  "homepage": "https://github.com/kentcdodds/slice-js#readme"
}
